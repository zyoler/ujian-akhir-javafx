<?import javafx.scene.layout.*?>
<VBox fx:id="containerBeranda" styleClass="spacing" stylesheets="/style.css" xmlns="http://javafx.com/javafx/8.0.171"
      xmlns:fx="http://javafx.com/fxml/1" fx:controller="org.neophyte.Controller.BerandaController" >
    <padding>
        <Insets bottom="12.0" left="12.0" right="12.0" top="12.0"/>
    </padding>
    <HBox prefHeight="47.0" prefWidth="714.0" spacing="12.0" style="-fx-alignment: center;" VBox.vgrow="ALWAYS">
        <TextField prefHeight="31.0" prefWidth="538.0" HBox.hgrow="ALWAYS" fx:id="textFieldSearch" promptText="search..." />
    </HBox>
    <VBox VBox.vgrow="ALWAYS">
        <ScrollPane VBox.vgrow="ALWAYS" fitToWidth="true" styleClass="spacing, scroll" >
            <FlowPane fx:id="movie" styleClass="flowjar"/>
        </ScrollPane>
    </VBox>
</VBox>
