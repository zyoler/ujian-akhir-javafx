<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<VBox prefHeight="200.0" prefWidth="400.0" style="-fx-background-color: black;" styleClass="container" stylesheets="/style.css" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="org.rental.controller.CreateMobilController">
    <VBox alignment="CENTER">
        <HBox prefHeight="41.0" prefWidth="376.0">
            <Label prefHeight="17.0" prefWidth="355.0" textFill="#ebe9e9">
            <font>
               <Font size="5.0" />
            </font></Label>
        </HBox>
        <Label alignment="CENTER" contentDisplay="CENTER" textFill="#eeecec">ID MOBIL :</Label>
        <TextField fx:id="mobilId" alignment="CENTER" promptText="Masukan ID Mobil" />
      <VBox.margin>
         <Insets bottom="6.0" left="12.0" right="12.0" top="6.0" />
      </VBox.margin>
    </VBox>
    <VBox alignment="CENTER" VBox.vgrow="ALWAYS">
        <Label textFill="#f8f3f3" VBox.vgrow="ALWAYS">MERK MOBIL:</Label>
        <TextField fx:id="merkMobil" alignment="CENTER" promptText="Masukan Merk Mobil" />
      <VBox.margin>
         <Insets bottom="6.0" left="12.0" right="12.0" top="6.0" />
      </VBox.margin>
    </VBox>
    <VBox alignment="CENTER" VBox.vgrow="ALWAYS">
        <Label textFill="#f5f4f4">NO POLISI :</Label>
        <TextField fx:id="noPolisi" alignment="CENTER" promptText="Masukan No Polisi" />
      <VBox.margin>
         <Insets bottom="6.0" left="12.0" right="12.0" top="6.0" />
      </VBox.margin>
    </VBox>
    <VBox alignment="CENTER" VBox.vgrow="ALWAYS">
        <Label alignment="CENTER" contentDisplay="CENTER" textFill="#fffdfd">HARGA SEWA :</Label>
        <TextField fx:id="hargaSewa" alignment="CENTER" promptText="Masukan Harga Sewa" />
      <VBox.margin>
         <Insets bottom="6.0" left="12.0" right="12.0" top="6.0" />
      </VBox.margin>
    </VBox>
    <VBox alignment="CENTER" VBox.vgrow="ALWAYS">
        <Label textFill="#fcf6f6">TIPE MOBIL :</Label>
        <TextField fx:id="tipeMobil" alignment="CENTER" promptText="Masukan Tipe Mobil" />
      <VBox.margin>
         <Insets bottom="6.0" left="12.0" right="12.0" top="6.0" />
      </VBox.margin>
    </VBox>
    <VBox alignment="CENTER" VBox.vgrow="ALWAYS">
        <Label textFill="#f8f0f0">TAHUN PEMBUATAN :</Label>
        <TextField fx:id="tahunPembuatan" alignment="CENTER" promptText="Masukan Tahun Pembuatan" />
      <VBox.margin>
         <Insets bottom="6.0" left="12.0" right="12.0" top="6.0" />
      </VBox.margin>
    </VBox>
    <VBox alignment="CENTER" VBox.vgrow="ALWAYS">
        <Label textFill="#f8f3f3">STATUS :</Label>
        <TextField fx:id="status" alignment="CENTER" promptText="Masukan Status Mobil" VBox.vgrow="ALWAYS">
         <VBox.margin>
            <Insets bottom="6.0" left="12.0" right="12.0" top="6.0" />
         </VBox.margin></TextField>
      <VBox.margin>
         <Insets />
      </VBox.margin>
    </VBox>

    <HBox alignment="CENTER" VBox.vgrow="ALWAYS">
      <VBox prefHeight="49.0" prefWidth="131.0">
         <children>
            <Button fx:id="saveButton" mnemonicParsing="false" prefHeight="25.0" prefWidth="67.0" style="-fx-background-color: blue;" text="Simpan" textFill="#fffafa" VBox.vgrow="ALWAYS">
               <VBox.margin>
                  <Insets bottom="12.0" left="6.0" top="16.0" />
               </VBox.margin>
            </Button>
         </children>
      </VBox>
      <VBox alignment="TOP_RIGHT" prefHeight="200.0" prefWidth="100.0" HBox.hgrow="ALWAYS">
         <children>
            <Button fx:id="cancelButton" contentDisplay="RIGHT" mnemonicParsing="false" prefHeight="25.0" prefWidth="52.0" style="-fx-background-color: orange;" text="Batal" textFill="#f8f8f8" VBox.vgrow="ALWAYS">
               <VBox.margin>
                  <Insets />
               </VBox.margin>
            </Button>
         </children>
         <HBox.margin>
            <Insets bottom="12.0" right="6.0" top="16.0" />
         </HBox.margin>
      </VBox>
    </HBox>
</VBox>
