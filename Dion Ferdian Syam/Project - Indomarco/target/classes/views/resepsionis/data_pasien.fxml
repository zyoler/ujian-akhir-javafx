<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.String?>
<?import java.net.URL?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>

<VBox alignment="CENTER_RIGHT" maxWidth="1.7976931348623157E308" prefHeight="400.0" prefWidth="750.0" spacing="20.0" HBox.hgrow="ALWAYS" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="indomarco.controllers.resepsionis.datapasien.DataPasienController">
   <styleClass>
      <String fx:value="container" />
      <String fx:value="sub-konten" />
   </styleClass>
   <children>
      <Label maxWidth="1.7976931348623157E308" prefHeight="17.0" prefWidth="224.0" styleClass="sub-header" text="Pasien terdaftar" VBox.vgrow="ALWAYS" />
      <Label fx:id="pasienTerdaftar" text="Label" />
      <HBox maxWidth="1.7976931348623157E308" prefHeight="100.0" VBox.vgrow="ALWAYS">
         <children>
            <TableView fx:id="pasienTableView" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" tableMenuButtonVisible="true" HBox.hgrow="ALWAYS">
              <columns>
                  <TableColumn fx:id="no" maxWidth="1000.0" prefWidth="75.0" text="No">
                     <styleClass>
                        <String fx:value="cell-pasien" />
                        <String fx:value="cell-center" />
                     </styleClass>
                  </TableColumn>
                <TableColumn fx:id="nik" maxWidth="4000.0" prefWidth="147.0" text="NIK">
                     <styleClass>
                        <String fx:value="cell-pasien" />
                        <String fx:value="cell-center" />
                     </styleClass></TableColumn>
                <TableColumn fx:id="namaLengkap" prefWidth="120.0" styleClass="cell-pasien" text="Nama pasien">
                </TableColumn>
                  <TableColumn fx:id="tanggalLahir" maxWidth="3000.0" prefWidth="75.0" text="Tanggal lahir">
                     <styleClass>
                        <String fx:value="cell-pasien" />
                        <String fx:value="cell-center" />
                     </styleClass></TableColumn>
                 <TableColumn fx:id="tanggalDaftar" maxWidth="3000.0" prefWidth="75.0" text="Tanggal daftar">
                     <styleClass>
                        <String fx:value="cell-pasien" />
                        <String fx:value="cell-center" />
                     </styleClass></TableColumn>
              </columns>
               <columnResizePolicy>
                  <TableView fx:constant="CONSTRAINED_RESIZE_POLICY" />
               </columnResizePolicy>
            </TableView>
         </children>
      </HBox>
   </children>
   <stylesheets>
      <URL value="@../../css/master.css" />
      <URL value="@../../css/data-pasien.css" />
   </stylesheets>
</VBox>
